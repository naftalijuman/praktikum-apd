<?xml version="1.0"?>
<flowgorithm fileversion="4.2">
    <attributes>
        <attribute name="name" value="2509106109-NavtalyJuman-PT-3"/>
        <attribute name="authors" value="LENOVO"/>
        <attribute name="about" value=""/>
        <attribute name="saved" value="2025-09-30 06.00.34 PM"/>
        <attribute name="created" value="TEVOT1ZPO0xBUFRPUC1KQU5GS1YxQTsyMDI1LTA5LTI5OzExLjUxLjQ2IFBNOzI4MDU="/>
        <attribute name="edited" value="TEVOT1ZPO0xBUFRPUC1KQU5GS1YxQTsyMDI1LTA5LTMwOzA2LjAwLjM0IFBNOzU7MjkwNA=="/>
    </attributes>
    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="User, Pasword, Nama, NIM, Fitur, Paket, Pilihan" type="String" array="False" size=""/>
            <declare name="BiayaLangganan, Admin, Total" type="Real" array="False" size=""/>
            <assign variable="User" expression="&quot;Navtaly Juman&quot;"/>
            <assign variable="Pasword" expression="&quot;2509106109&quot;"/>
            <assign variable="BiayaLangganan" expression="1500000"/>
            <output expression="&quot;LOGIN APLIKASI STREAMING MUSIK&quot;" newline="True"/>
            <output expression="&quot;Masukkan Nama Anda&quot;" newline="True"/>
            <input variable="Nama"/>
            <output expression="&quot;Masukkan NIM Anda&quot;" newline="True"/>
            <input variable="NIM"/>
            <if expression="Nama == User and NIM == Pasword">
                <then>
                    <output expression="&quot;Login Berhasil&quot;" newline="True"/>
                    <output expression="&quot;PILIHAN PAKET LANGGANAN&quot;" newline="True"/>
                    <input variable="Pilihan"/>
                    <if expression="Pilihan = &quot;1&quot;">
                        <then>
                            <assign variable="Admin" expression="0.01"/>
                            <assign variable="Fitur" expression="&quot;Akses dasar ke lagu-lagu populer&quot;"/>
                            <assign variable="Paket" expression="&quot;Bronze&quot;"/>
                        </then>
                        <else>
                            <if expression="Pilihan = &quot;2&quot;">
                                <then>
                                    <assign variable="Admin" expression="0.03"/>
                                    <assign variable="Paket" expression="&quot;Akses lagu premium dan playlist kustom&quot;"/>
                                    <assign variable="Paket" expression="&quot;Silver&quot;"/>
                                </then>
                                <else>
                                    <if expression="Pilihan =&quot;3&quot;">
                                        <then>
                                            <assign variable="Admin" expression="0.05"/>
                                            <assign variable="Fitur" expression="&quot;Akses lagu premium,playlist kustom, dan mode offline&quot;"/>
                                            <assign variable="Paket" expression="&quot;Gold&quot;"/>
                                        </then>
                                        <else>
                                            <if expression="Pilihan =&quot;4&quot;">
                                                <then>
                                                    <assign variable="Admin" expression="0.07"/>
                                                    <assign variable="Fitur" expression="&quot;Akses semua fitur, playlist kustom, mode offline, konten ekslusif artis&quot;"/>
                                                    <assign variable="Paket" expression="&quot;Platinum&quot;"/>
                                                </then>
                                                <else>
                                                    <output expression="&quot;Pilihan paket tidak valid!&quot;" newline="True"/>
                                                </else>
                                            </if>
                                        </else>
                                    </if>
                                </else>
                            </if>
                        </else>
                    </if>
                    <assign variable="Total" expression="BiayaLangganan +(BiayaLangganan * Admin)"/>
                    <output expression="&quot;Rician pembayaran&quot;" newline="True"/>
                    <output expression="&quot;Paket: &quot; + Paket" newline="True"/>
                    <output expression="&quot;Biaya Paket: Rp &quot; + BiayaLangganan" newline="True"/>
                    <output expression="&quot;Biaya Admin: &quot; + (BiayaLangganan * Admin)" newline="True"/>
                    <output expression="&quot;Total Bayar: Rp &quot; + Total" newline="True"/>
                    <output expression="&quot;Terima kasih telah berlangganan di aplikasi streaming musik kami&quot;" newline="True"/>
                </then>
                <else>
                    <output expression="&quot;Login gagal! Nama atau NIM salah!&quot;" newline="True"/>
                    <output expression="&quot;Terima kasih, silahkan coba lagi nanti&quot;" newline="True"/>
                </else>
            </if>
        </body>
    </function>
</flowgorithm>
