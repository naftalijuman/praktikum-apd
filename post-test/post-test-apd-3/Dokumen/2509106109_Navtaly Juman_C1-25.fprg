    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="lamaparkir, biaya" type="Integer" array="False" size=""/>
            <output expression="&quot;berapa lama anda parkir&quot;" newline="True"/>
            <input variable="lamaparkir"/>
            <if expression="lamaparkir &lt;= 2">
                <then>
                    <assign variable="biaya" expression="5000"/>
                </then>
                <else>
                    <assign variable="biaya" expression="5000+(lamaparkir-2)*2000"/>
                    <if expression="biaya &gt;20000">
                        <then>
                            <assign variable="biaya" expression="20000"/>
                        </then>
                        <else/>
                    </if>
                </else>
            </if>
            <output expression="&quot;biaya parkir anda=rp&quot;&amp;biaya" newline="True"/>
        </body>
    </function>
