    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="Nama, NIM" type="String" array="False" size=""/>
            <declare name="Harga" type="Integer" array="False" size=""/>
            <declare name="diskonAcer, hargaAcer, diskonAsus, hargaAsus, diskonLenovo, hargaLenovo" type="Real" array="False" size=""/>
            <output expression="&quot;masukkan Nama lengkap anda&quot;" newline="True"/>
            <input variable="Nama"/>
            <output expression="&quot;masukkan NIM anda&quot;" newline="True"/>
            <input variable="NIM"/>
            <output expression="&quot;masukkan harga Laptop&quot;" newline="True"/>
            <input variable="Harga"/>
            <assign variable="diskonAcer" expression="harga * 0.05"/>
            <assign variable="hargaAcer" expression="Harga - diskonAcer"/>
            <assign variable="diskonAsus" expression="Harga * 0.07"/>
            <assign variable="hargaAsus" expression="harga - diskonAsus"/>
            <assign variable="diskonLenovo" expression="harga * 0.10"/>
            <assign variable="hargaLenovo" expression="harga - diskonLenovo"/>
            <output expression="Nama  &amp;&quot;dengan NIM&quot; &amp; nim &amp;&quot;ingin membeli Laptop seharga Rp&quot; &amp; harga" newline="True"/>
            <output expression="&quot;Berikut harga laptop setelah diskon tiap merek&quot;" newline="True"/>
            <output expression="&quot;Merek Acer, harga setelah diskon 5% Rp&quot;&amp;hargaAcer" newline="True"/>
            <output expression="&quot;Merek Asus, harga setelah diskon 7% Rp&quot;&amp; hargaAsus" newline="True"/>
            <output expression="&quot;Merek Lenovo, harga setalah diskon 10% Rp&quot;&amp; hargaLenovo" newline="True"/>
        </body>
