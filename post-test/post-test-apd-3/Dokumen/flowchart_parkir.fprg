<?xml version="1.0"?>
<flowgorithm fileversion="2.11">
    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="lama_parkir" type="Integer" array="False" size=""/>
            <declare name="biaya" type="Integer" array="False" size=""/>
            <output expression="&quot;Masukkan lama parkir (jam): &quot;" newline="True"/>
            <input variable="lama_parkir"/>
            <if expression="lama_parkir &lt;= 2">
                <then>
                    <assign variable="biaya" expression="5000"/>
                </then>
                <else>
                    <assign variable="biaya" expression="5000 + (lama_parkir - 2) * 2000"/>
                </else>
            </if>
            <if expression="biaya &gt; 20000">
                <then>
                    <assign variable="biaya" expression="20000"/>
                </then>
            </if>
            <output expression="&quot;Biaya parkir = Rp &quot; + biaya" newline="True"/>
        </body>
    </function>
</flowgorithm>
